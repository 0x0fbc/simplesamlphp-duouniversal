    "authors": [
        {
            "name": "Forest B. Crowley",
            "email": "fbc@lehigh.edu"
        }
    ],
    "require": {
        "simplesamlphp/composer-module-installer": "~1.0",
        "duosecurity/duo_universal_php": "1.0.1"
    },
    "config": {
        "allow-plugins": {
            "simplesamlphp/composer-module-installer": true
        }
    }
}
